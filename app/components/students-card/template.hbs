{{#paper-card as |card|}}
  {{#card.title as |title|}}
    {{#title.text as |text|}}
      {{#text.headline}}Students{{/text.headline}}
    {{/title.text}}
  {{/card.title}}
  {{#card.content}}
    {{#paper-list}}
      {{#each students as |student|}}
        {{#paper-item class="md-3-line"}}
          <img src={{student.avatar}} alt={{student.name}} class="md-avatar">
          <div class="md-list-item-text">
            <h3>{{student.name}}</h3>
            <p>{{student.age}}</p>
          </div>
        {{/paper-item}}
      {{/each}}
    {{/paper-list}}
  {{/card.content}}
{{/paper-card}}
